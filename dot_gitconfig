[http]
	sslVerify = False
[credential "https://git-codecommit.ap-northeast-1.amazonaws.com"]
	helper = !aws --region ap-northeast-1 codecommit credential-helper --profile default $@
	UseHttpPath = true
[credential]
	helper = cache
[push]
	default = simple
	recurseSubmodules = check
[core]
	editor = micro
	pager = delta
[pull]
	rebase = false
[delta]
	navigate = true
	side-by-side = true
	line-numbers = true
	light = true
[include]
	path = ~/.gitconfig.local
